{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "CSV Formatter Configuration",
      "properties": [
        {
          "widget-type": "select",
          "label": "Formats",
          "name": "format",
          "widget-attributes": {
            "values": [
              "DELIMITED",
              "EXCEL",
              "MYSQL",
              "RFC4180",
              "TDF"
            ],
            "default": "DELIMITED"
          }
        },
        {
          "widget-type": "select",
          "label": "Choose a delimiter",
          "name": "delimiter",
          "widget-attributes": {
            "values": [
              "CARET",
              "COMMA",
              "CTRL-A",
              "CTRL-B",
              "CTRL-C",
              "CTRL-D",
              "CTRL-E",
              "CTRL-F",
              "DOLLAR",
              "HASH",
              "STAR",
              "TAB",
              "VBAR"
            ],
            "default": "CTRL-A"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "schema",
      "label": "schema",
      "widget-type": "schema",
      "widget-attributes": {
        "schema-types": [
          "string"
        ],
        "schema-default-type": "string",
        "property-watch": "format"
      }
    }
  ]
}
